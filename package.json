{
  "name": "@forkzz/mercadopago",
  "version": "1.0.12",
  "description": "",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./browser": {
      "import": "./dist/browser.mjs",
      "require": "./dist/browser.js",
      "types": "./dist/browser.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/browser.ts --format cjs,esm --dts --clean",
    "dev": "tsup src/index.ts src/browser.ts --format cjs,esm --dts --watch",
    "lint": "tsc --noEmit",
    "prepublishOnly": "npm run lint && npm run build"
  },
  "keywords": [
    "mercadopago",
    "mercado-pago",
    "pagamentos",
    "pix",
    "sdk",
    "typescript",
    "payments",
    "brazil",
    "checkout",
    "subscriptions"
  ],
  "author": "isforkzz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/isforkzz/mercadopago"
  },
  "homepage": "https://github.com/isforkzz/mercadopago#readme",
  "bugs": {
    "url": "https://github.com/isforkzz/mercadopago/issues"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
